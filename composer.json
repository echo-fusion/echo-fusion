{
    "name": "echo-fustion/echo-fustion",
    "description": "structure of echoFusion php framework",
    "type": "project",
    "license": "MIT",
    "keywords": ["echoFustion", "skeleton","framework"],
    "support": {
        "issues": "https://github.com/echo-fusion/skeleton/issues",
        "source": "https://github.com/echo-fusion/skeleton"
    },
    "authors": [
        {
            "name": "Amir Shadanfar",
            "email": "a.shadanfar.it@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "echo-fusion/framework": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "echo-fusion/framework",
                "version": "dev-master",
                "source": {
                    "url": "../framework",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "dev-master",
        "echo-fusion/codestandard": "^1.0",
        "vimeo/psalm": "5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "EchoFusion\\": "/",
            "App\\": "/app/",
            "Modules\\": "/modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
